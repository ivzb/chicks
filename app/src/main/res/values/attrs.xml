<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="Theme">
        <!-- Navigation bar divider color, used by NavigationBarContentFrameLayout -->
        <attr name="chicksNavigationBarDividerColor" format="color" />

        <attr name="colorControlLight" format="color" />

        <attr name="sessionListKeyline" format="dimension" />

        <attr name="eventFilterViewStyle" format="reference" />

        <attr name="eventCardHeaderBackground" format="reference|color" />
    </declare-styleable>

    <declare-styleable name="NavigationBarContentFrameLayout">
        <attr name="navigationBarBorderColor" format="color" />
        <attr name="navigationBarBorderSize" format="dimension" />
    </declare-styleable>

    <declare-styleable name="HashtagChicksDecoration">
        <attr name="android:drawable" />
        <attr name="margin" format="dimension" />
    </declare-styleable>
</resources>